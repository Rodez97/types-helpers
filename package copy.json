{
  "name": "@cuttinboard-solutions/types-helpers",
  "version": "3.2.8",
  "description": "Main types and helper functions for cuttinboard",
  "main": "dist/compiled/index.cjs",
  "module": "dist/compiled/index.mjs",
  "source": "src/index.ts",
  "types": "dist/compiled/index.d.ts",
  "type": "module",
  "isLibrary": true,
  "files": [
    "dist/compiled/index.cjs",
    "dist/compiled/index.d.ts",
    "dist/compiled/index.mjs.map",
    "dist/compiled/index.mjs"
  ],
  "scripts": {
    "_clear": "rimraf dist/",
    "_tsc": "tsc",
    "_make-bundle": "rollup --config",
    "build": "npm run _clear && npm run _tsc && npm run _make-bundle",
    "test": "mocha",
    "deploy": "npm run build && npm publish",
    "generate-docs": "typedoc --out docs src/index.ts",
    "generate-docs:markdown": "typedoc --plugin typedoc-plugin-markdown --out markdownDocs src/index.ts"
  },
  "author": "Cuttinboard",
  "license": "ISC",
  "peerDependencies": {
    "date-fns": ">=2.29.3",
    "dayjs": ">=1.11.6",
    "dayjs-recur": ">=1.0.1",
    "lodash": ">=4.17.21"
  },
  "devDependencies": {
    "@optimize-lodash/rollup-plugin": "^4.0.3",
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "@rollup/plugin-commonjs": "^25.0.1",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/dayjs-recur": "^1.0.0",
    "@types/lodash": "^4.17.7",
    "@types/node": "^16.11.6",
    "deepmerge": "^4.3.1",
    "parcel": "^2.8.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.23.0",
    "rollup-plugin-dts": "^5.3.0",
    "tslib": "^2.5.2",
    "typedoc": "^0.24.7",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Cuttinboard-Solutions/types-helpers.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
